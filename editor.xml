<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="16" />
    <option name="SECONDARY_FONT_FAMILY" value="Monaco" />
  </component>
  <component name="EditorSettings">
    <option name="IS_CARET_INSIDE_TABS" value="true" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_INTENTION_BULB" value="false" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="IS_FOLDING_OUTLINE_SHOWN" value="false" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>